# Author: Evan Killion
# Title: Geospatial Analysis of McMenamin's Locations
# Purpose: Extract coordinates of McMenamin's locations from map on their website

### 1. Load necessary packages
library(tidyverse)
library(V8)
library(dplyr)
library(htmltools)
library(htmlwidgets)
library(httr)
library(jsonlite)

ct <- v8()

# This code uses the ggmap package for mapping in R
library(ggmap)

latlngbounds <- NULL
myLatLng1 <- list(lat = 45.535327, lng = -122.698563)
marker1 <- list(position = myLatLng1, title = 'Click to view 23rd Avenue Bottle Shop')

# Simulating a click event
onClickMarker1 <- function() {
  browseURL('/23rd-avenue-bottle-shop')
}

# Assuming latlngbounds is a list to store positions
latlngbounds <- c(latlngbounds, myLatLng1)
listMarkers <- list()
listMarkers[[1]] <- marker1

myLatLng2 <- list(lat = 45.522791, lng = -122.683361)
marker2 <- list(position = myLatLng2, title = "Click to view Al's Den")

# Simulating a click event
onClickMarker2 <- function() {
  browseURL('/crystal-hotel/als-den')
}

latlngbounds <- c(latlngbounds, myLatLng2)
listMarkers[[2]] <- marker2


